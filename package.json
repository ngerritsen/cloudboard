{
  "name": "cloudboard",
  "version": "2.7.0",
  "description": "A shared soundboard. ï¿½",
  "main": "server/index.js",
  "engines": {
    "node": ">16"
  },
  "scripts": {
    "start": "node server",
    "serve": "concurrently 'nodemon server' 'npm:watch'",
    "copy-static": "mkdir -p public; cp -r sounds public/",
    "watch": "NODE_ENV=development webpack-dev-server",
    "build": "npm run copy-static; NODE_ENV=production webpack",
    "test": "eslint .",
    "generate": "mkdir -p tools/output; rm -Rf tools/output/*; node ./tools/sound-generator.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/ngerritsen/cloudboard"
  },
  "keywords": [
    "soundboard",
    "cloud",
    "nederlands",
    "nl"
  ],
  "author": "Niels Gerritsen",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/ngerritsen/cloudboard/issues"
  },
  "homepage": "https://gitlab.com/ngerritsen/cloudboard#readme",
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "redux-map-reducers": "^1.0.1",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.5.4",
    "shortid": "^2.2.6",
    "socker.io": "^0.0.0-alpha-beta",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "css-loader": "^6.6.0",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.10.0",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "nodemon": "^2.0.15",
    "pre-commit": "^1.1.3",
    "redux-logger": "^3.0.6",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "nodemonConfig": {
    "ignore": [
      "src/*"
    ]
  }
